// Завдання 1
let user = {};          // Створити порожній об'єкт
user.name = "Марк";     // Додати властивість name
user.surname = "Сміт";  // Додати властивість surname
user.name = "Тарас";    // Змінити значення name
delete user.name;       // Видалити властивість name



// Завдання 2
function isEmpty(obj) {
  for (let key in obj) {
    return false;  // Якщо є хоча б одна властивість
  }
  return true;     // Якщо об'єкт порожній
}

// Приклад використання:
let emptyObj = {};
console.log(isEmpty(emptyObj)); // true

let nonEmptyObj = {test: 1};
console.log(isEmpty(nonEmptyObj)); // false



// Завдання 3
let salaries = {
  Jarik: 1000,
  Anna: 1600,
  Miko: 1300
};

let sum = 0;
for (let key in salaries) {
  sum += salaries[key];
}

console.log(sum); // 3900



// Завдання 4
function multiplyNumeric(obj) {
  for (let key in obj) {
    if (typeof obj[key] === 'number') {
      obj[key] *= 2;
    }
  }
}

// Приклад використання:
let menu = {
  width: 200,
  height: 300,
  title: "Меню"
};

multiplyNumeric(menu);

console.log(menu);
// { width: 400, height: 600, title: "Меню" }
